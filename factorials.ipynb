   "source": [
    "def factorial(n):\n",
    "    if n < 1:\n",
    "        return 1\n",
    "    else:\n",
    "        result = n * factorial( n - 1 )\n",
    "        print(\"{}! = {}\".format(str(n), str(result)))\n",
    "        return result"
   ]
